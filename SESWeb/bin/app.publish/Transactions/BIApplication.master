        <telerik:RadCodeBlock runat="server" ID="rcbGeneral">
            <script type="text/javascript">
                $ = $telerik.$;
                function HideCustomLoading() {
                    try {
                        setTimeout(function () { $("#CustomLoadingPanel").css('display', 'none'); }, 500);
                    } catch (e) {

                    }
                }

                function ShowCustomLoading() {
                    try {
                        centerElementOnScreen();
                        $("#CustomLoadingPanel").css('display', 'block');
                    } catch (e) {

                    }
                }

                ShowCustomLoading();

                function centerElementOnScreen() {
                    try {
                        var elements = $('.lPanel');
                        var scrollTop = document.body.scrollTop;
                        var scrollLeft = document.body.scrollLeft;
                        var viewPortHeight = document.body.clientHeight;
                        var viewPortWidth = document.body.clientWidth;
                        if (document.compatMode == "CSS1Compat") {
                            viewPortHeight = document.documentElement.clientHeight;
                            viewPortWidth = document.documentElement.clientWidth;
                            if (!$telerik.isSafari) {
                                scrollTop = document.documentElement.scrollTop;
                                scrollLeft = document.documentElement.scrollLeft;
                            }
                        }
                        var topOffset = Math.ceil(viewPortHeight / 2 - $(elements[0]).prop('clientHeight') / 2);
                        var leftOffset = Math.ceil(viewPortWidth / 2 - $(elements[0]).prop('clientWidth') / 2);
                        var top = scrollTop + topOffset - 70;
                        var left = scrollLeft + leftOffset - 100;
                        $(elements).css('position', 'absolute');
                        $(elements).css('top', top);
                        $(elements).css('left', left);
                    }
                    catch (err) { }
                }

                function txtSearchBox_onClientLoad(sender, args) {
                    sender._requestDelay = 1000;
                }

                function counter(field, countfield, maxlimit) {
                    if ('#' + countfield == null) { return; }
                    $telerik.$('#' + countfield).show();
                    $telerik.$('#' + countfield).html(maxlimit - field.value.length + " Characters Remaining");
                    if (maxlimit === field.value.length) {
                        $telerik.$('#' + countfield).css('color', 'red');
                        setTimeout(function () { $telerik.$('#' + countfield).hide(); }, 1000);
                    }
                    else {
                        $telerik.$('#' + countfield).css('color', 'green');
                    }
                }

                function ShowSideMenu(show) {
                    var ua = window.navigator.userAgent;
                    var msie = ua.indexOf("MSIE ");
                    if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./))  // If Internet Explorer, return version number
                    {
                        if (show == true)
                            $(".SideMenuList").show();
                        else
                            $(".SideMenuList").hide();
                    }
                }

                var BISideNavOpen = false;
                $(document).ready(function () {
                    if (cookies.test()) {
                        if (cookies.get("BISideNavOpen") != null) {
                            BISideNavOpen = cookies.get("BISideNavOpen");
                            if (BISideNavOpen == true || BISideNavOpen == "true") {
                                $(".SideMenuList").removeClass("hiddensidenav");
                                $(".main-btns").css("padding-right", "300px");
                                $("#pnlMainContent").css("marginLeft", "310px");
                                $(".PinMenu").addClass("MenuPinned");
                                $(".PinMenu").prop('title', "Unpin menus");
                                ShowSideMenu(true);
                            }
                            else {
                                $(".SideMenuList").addClass("hiddensidenav");
                                $(".main-btns").css("padding-right", "0px");
                                $("#pnlMainContent").css("marginLeft", "0px");
                                $(".PinMenu").removeClass("MenuPinned");
                                $(".PinMenu").prop('title', "Pin menus");
                                ShowSideMenu(false);
                            }
                        }
                        else {
                            $(".SideMenuList").addClass("hiddensidenav");
                            $(".main-btns").css("padding-right", "0px");
                            $("#pnlMainContent").css("marginLeft", "0px");
                            $(".PinMenu").removeClass("MenuPinned");
                            $(".PinMenu").prop('title', "Pin menus");
                            ShowSideMenu(false);
                        }
                    }
                    else {
                        $(".SideMenuList").addClass("hiddensidenav");
                        $(".main-btns").css("padding-right", "0px");
                        $("#pnlMainContent").css("marginLeft", "0px");
                        $(".PinMenu").removeClass("MenuPinned");
                        $(".PinMenu").prop('title', "Pin menus");
                        ShowSideMenu(false);
                    }

                });
                $('.hamburger-click').click(function () {
                    if ($(".SideMenuList").hasClass("hiddensidenav")) {
                        $(".SideMenuList").removeClass("hiddensidenav");
                        $("#pnlMainContent").css("marginLeft", "310px");
                        $(".main-btns").css("padding-right", "300px");
                        ShowSideMenu(true);
                    }
                    else {
                        $(".SideMenuList").addClass("hiddensidenav");
                        $(".main-btns").css("padding-right", "0px");
                        $("#pnlMainContent").css("marginLeft", "0px");
                        ShowSideMenu(false);
                    }
                });

                function PinMenu() {
                    BISideNavOpen = cookies.get("BISideNavOpen");
                    if (BISideNavOpen == false || BISideNavOpen == "false") {
                        $(".PinMenu").addClass("MenuPinned");
                        $(".PinMenu").prop('title', "Unpin menus");
                        ShowSideMenu(true);
                        UpdateSideNavCookie(true);
                        BISideNavOpen = true;
                    }
                    else {
                        $(".PinMenu").removeClass("MenuPinned");
                        $(".PinMenu").prop('title', "Pin menus");
                        $(".SideMenuList").addClass("hiddensidenav");
                        $(".main-btns").css("padding-right", "0px");
                        $("#pnlMainContent").css("marginLeft", "0px");
                        ShowSideMenu(false);
                        UpdateSideNavCookie(false);
                        BISideNavOpen = false;
                    }
                }

                function UpdateSideNavCookie(newValue) {
                    cookies.set('BISideNavOpen', newValue, { expires: 10000 });
                }

                $(document).ready(function () {
                    try {
                        HideCustomLoading();
                    } catch (e) { }
                });
            </script>
        </telerik:RadCodeBlock>
