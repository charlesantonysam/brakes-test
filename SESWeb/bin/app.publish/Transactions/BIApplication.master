<asp:Content ID="phNotification" ContentPlaceHolderID="cphNotification" runat="server">
    <a href="" runat="server" id="lnkNotification">
        <img src="../Resources/pixel/images/icons/notification.png">
        <span runat="server" id="lblNotificationBadge"></span></a>
    <asp:Label runat="server" ID="lblNotificationCount"></asp:Label>

    <ul class="notify-menu divse-menu section-left" id="ulNotification" style="overflow: auto; vertical-align: top;">
        <ul class="mint-lis">
            <telerik:RadListView runat="server" ID="rlvNotification" AllowPaging="false" OnItemCommand="rlvNotification_ItemCommand"
                DataKeyNames="ApplicationName,NotificationId,RedirectionPage,Querystring,CreatedDate,PortalURL,ApplicationClientId,VirtualDirectory,PageName" AllowMultiItemSelection="false">
                <ItemTemplate>
                    <li>
                        <asp:LinkButton runat="server" ID="btnNotification" CommandName="OpenTask" OnClientClick="ShowCustomLoading();">
                                            <h5><%#Eval("MessageTitle") %></h5>
                            <p><%#Eval("MessageDetail") %></p>
                        </asp:LinkButton>
                    </li>
                </ItemTemplate>
            </telerik:RadListView>

        </ul>
    </ul>
    <telerik:RadScriptBlock runat="server" ID="rcbNotification">
        <script type="text/javascript">
            function UpdateNotificationViewStatus() {
                try {
                    var lblNotification = '#<%=lblNotificationCount.ClientID%>';
                    var lblNotificationBadge = '#<%=lblNotificationBadge.ClientID%>';
                    var TimeStamp = '<%=NotificationTimeStamp.ToString("dd/MMM/yyyy HH:mm:ss")%>';
                    $telerik.$.ajax({
                        url: 'Notifications.aspx/UpdateNotificationViewStatus',
                        type: "POST",
                        dataType: "json",
                        data: "{'TimeStamp':'" + TimeStamp + "'}",
                        contentType: "application/json; charset=utf-8",
                        success: function (data) {
                            $(lblNotification).css('display', 'none');
                            $(lblNotificationBadge).css('display', 'none');
                        }
                    });
                } catch (e) {

                }
            }
            $telerik.$(document).ready(function () {
                try {
                    if ($($('#ulNotification')).find('a').length > 0) {
                        $telerik.$('#ulNotification').height($telerik.$(window).height() - 30);
                    }
                } catch (e) {
                    $telerik.$('#ulNotification').height($telerik.$(window).height() - 30);
                }

            });
        </script>
    </telerik:RadScriptBlock>
</asp:Content>
